{
  "name": "Password Security Test Flow",
  "description": "Test flow for password field masking and security",
  "startUrl": "https://secure.example.com/login",
  "params": [
    "CURRENT_PASSWORD",
    "NEW_PASSWORD",
    "CONFIRM_NEW_PASSWORD",
    "MASTER_PASS",
    "SECRET_CODE"
  ],
  "steps": [
    {
      "action": "type",
      "target": "#current-password",
      "value": "{{CURRENT_PASSWORD}}"
    },
    {
      "action": "type",
      "target": "#new-password",
      "value": "{{NEW_PASSWORD}}"
    },
    {
      "action": "type",
      "target": "#confirm-password",
      "value": "{{CONFIRM_NEW_PASSWORD}}"
    },
    {
      "action": "type",
      "target": "#master-pass",
      "value": "{{MASTER_PASS}}"
    },
    {
      "action": "type",
      "target": "#secret-code",
      "value": "{{SECRET_CODE}}"
    },
    {
      "action": "click",
      "target": "#change-password-btn",
      "value": ""
    }
  ],
  "successCheck": ".password-change-success"
}